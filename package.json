{
  "name": "flashlight-icli",
  "version": "0.0.20",
  "description": "Interactive command line interface for flashlight",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc && chmod +x dist/index.js",
    "icli": "node dist/index.js",
    "clean-npx-cache": "npx clear-npx-cache",
